{
  "$schema": "https://opencode.ai/config.json",
  "share": "disabled",
  "model": "quiqgpt/litellm_manifold_pipeline.gemini-3-pro",
  "disabled_providers": ["opencode"],
  "logLevel": "DEBUG",
  "provider": {
    "quiqgpt": {
      "name": "QuiqGPT",
      "npm": "@ai-sdk/openai-compatible",
      "options": {
        "baseURL": "https://quiqgpt-api.quiq.sh/openai",
        "litellmProxy": true,
      },
      "models": {
        "litellm_manifold_pipeline.claude-4.6-sonnet": {
          "id": "litellm_manifold_pipeline.claude-4.6-sonnet",
          "name": "Claude 4.6 Sonnet",
          "options": {
            "litellmProxy": true,
          },
          "limit": {
            "context": 400000,
            "output": 64000
          }
        },
        "litellm_manifold_pipeline.gemini-3-pro": {
          "id": "litellm_manifold_pipeline.gemini-3-pro",
          "name": "Gemini 3 Pro",
          "options": {
            "litellmProxy": true,
          },
          "limit": {
            "context": 400000,
            "output": 64000
          }
        }
      }
    }
  },
  "permission": {
    "bash": {
      "*": "ask",
      "rm *": "deny",
      "git *": "ask",
      "grep *": "allow",
    },
    "edit": {
      "*": "ask",
    },
    "read": {
      "*": "allow"
    },
    "list": {
      "*": "allow"
    },
    "task": {
      "*": "allow"
    },
    "todoread": "allow",
    "todowrite": "allow",
    "webfetch": "allow",
    "websearch": "allow",
    "external_directory": "ask",
    "doom_loop": "deny"
  }
}
